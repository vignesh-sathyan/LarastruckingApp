@model LarastruckingApp.ViewModel.Customer.CustomerQuotesViewModel
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-12 col-md-12 mt-2">
        <div class="white-bg customFormWrap">
            <div class="customTableWrapper">
                <div id="">
                    <table class="table-bordered dataTable table-striped cf w-100 tblChange-bgcolor" id="tblCutomerDetails">
                        <thead>
                            <tr>
                                <th> Sl. No. </th>
                                <th>Status </th>
                                <th>Loads</th>
                                <th>Air Waybill</th>
                                <th>Pickup Date</th>
                                <th>Delivery DateTime</th>
                                @*<th> Shipment Ref. No. </th>*@
                                <th> Driver Name </th>
                                <th>Equipment</th>
                                @*<th>Track</th>*@
                                <th> View </th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>

        </div>
    </div>
</div>


@Scripts.Render("~/bundles/CustomerDashboard")
@*<script src="~/CustomScript/customerJs/CustomerDashboard.js"></script>*@